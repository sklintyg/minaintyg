{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "es3": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "nonbsp": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxstatements": 150,
    "maxcomplexity": 10,
    "maxlen": 150,
    "maxerr": 10000,
    "browser": false,
    "devel": false,

    "globals": {
        "afterEach": false,
        "beforeEach": false,
        "describe": false,
        "expect": false,
        "it": false,
        "jasmine": false,
        "angular": false,
        "moment": false,
        "$": false,
        "MODULE_CONFIG": false
    }
}
