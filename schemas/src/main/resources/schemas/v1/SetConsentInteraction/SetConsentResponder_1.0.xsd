  <element name="SetConsentResponse" type="tns:SetConsentResponseType" />
  <element name="SetConsentRequest" type="tns:SetConsentRequestType" />

  <complexType name="SetConsentResponseType">
    <sequence>
      <element name="result" type="pref2:ResultOfCall" />
      <any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </sequence>
  </complexType>
  
  <complexType name="SetConsentRequestType">
    <sequence>
      <element name="personnummer" type="string" minOccurs="1" maxOccurs="1" />
      <element name="consent-given" type="boolean" minOccurs="1" maxOccurs="1" />
      <any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded" />
    </sequence>
  </complexType>
</schema>